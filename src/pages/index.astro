<script is:inline>
	function changeelmt(d, div_id,h_id) {
	  var TextElements = document.getElementsByClassName("aa");
  
	  for (var i = 0, max = TextElements.length; i < max; i++) {
		TextElements[i].style.color = "rgb(30 58 138 / var(--tw-text-opacity))";
	  }
	  document.getElementById(h_id).style.color = "rgb(249 115 22 / var(--tw-bg-opacity))";
	  for (let element of document.getElementsByClassName("divcls")) {
		element.style.display = "none";
	  }
	  document.getElementById(div_id).style.display = "flex";
  
	}
  </script>
  
