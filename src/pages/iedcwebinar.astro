---
import Layout from "../layouts/Layout.astro";

---

<Layout title="">
    <main class="">
		<div class="flex -z-10 bg-cover object-center min-w-full bg-center bg-no-repeat w-full pt-24" style="background-image:url('/img/AdobeStock_582485671.jpeg')">
		<div class="container my-auto text-center py-7">
			<h1 class="md:text-7xl font-extrabold text-blue-900 text-5xl">
                INSPIRE 2 INNOVATE
			</h1>
		</div>
		</div>
	</main>
    <section class=" pb-24">
        <div  style="background-image: url('img/microphone-headphones-for-singing-recording-stud-2022-05-18-04-17-17-utc.jpg');" class=" object-contain">
            <div class="px-10 py-10 text-white text-center space-y-4">
                <p class=" md:text-4xl font-extrabold text-3xl justify-center">INNOVATION, TECHNOLOGY, AND ENTREPRENEURSHIP </p>
                <p class="md:text-3xl text-2xl justify-center">WEBINARS BEYOND HORIZONS.</p>
